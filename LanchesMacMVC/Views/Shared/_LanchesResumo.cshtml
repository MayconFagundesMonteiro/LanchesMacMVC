@model Lanche
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="img-thumbnail">
        <img src=@Model.ImagemUrl class="img-fluid" />
    </div>
    <div class="figure-caption">
        <h3 class="text-right">@Model.Preco.ToString("c")</h3>
        <h3>Lanche : @Model.Nome</h3>
        <p>@Model.DescricaoCurta</p>
    </div>
</div>